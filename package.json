{
  "name": "webhook-contentful-app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=14.0.0 <15.0.0",
    "npm": "^6.14.15"
  },
  "dependencies": {
    "@contentful/field-editor-single-line": "^1.1.2",
    "@contentful/field-editor-test-utils": "^1.1.2",
    "@contentful/forma-36-fcss": "0.3.5",
    "@contentful/forma-36-react-components": "^3.100.6",
    "@contentful/forma-36-tokens": "^0.11.2",
    "@emotion/core": "^11.0.0",
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.42",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "contentful-ui-extensions-sdk": "^4.6.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^8.0.1",
    "jest-environment-jsdom-sixteen": "^2.0.0",
    "prettier": "^2.6.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "CI=true react-scripts test --env=jsdom-sixteen --verbose --colors",
    "test:watch": "react-scripts test --env=jsdom-sixteen --verbose",
    "lint": "eslint ./src/**/*.*",
    "lint:fix": "eslint ./src/**/*.* --fix",
    "format": "prettier --check src/**/*.*",
    "format:fix": "prettier --write src/**/*.*",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "plugin:react/recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "settings": {
      "react": {
        "version": "detect"
      }
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
